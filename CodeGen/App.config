<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="variables" type="PerfectStorm.CodeGenLibrary.XmlNodeConfigSection, CodeGenLibrary" />
    <section name="logging" type="PerfectStorm.CodeGenLibrary.XmlNodeConfigSection, CodeGenLibrary" />
    <section name="transform" type="PerfectStorm.CodeGenLibrary.XmlNodeConfigSection, CodeGenLibrary" />
    <section name="plugins" type="PerfectStorm.CodeGenLibrary.XmlNodeConfigSection, CodeGenLibrary" />
    <section name="filters" type="PerfectStorm.CodeGenLibrary.XmlNodeConfigSection, CodeGenLibrary" />
  </configSections>
  <variables>
    <variable name="CodeGen.Tool">This was generated by PerfectStorm.CodeGen.</variable>
  </variables>
  <logging filename="changes.txt" />
  <!-- Possible values are "" or "mono" -->
  <transform transformAdapter="" />
  <filters>
    <filter assembly="CodeGenLibrary" class="PerfectStorm.CodeGenLibrary.SupressUnicodePrefixFilter" />
    <filter assembly="CodeGenLibrary" class="PerfectStorm.CodeGenLibrary.HeaderCopyFilter" startMarker="XXX" endMarker="YYY" />
  </filters>  
  <plugins>
    <logger assembly="CodeGenLibrary" class="PerfectStorm.CodeGenLibrary.FileLogger" />
  </plugins>
</configuration>